{
    "name": "red-hat-kickstarts-app",
    "version": "0.1.0",
    "private": true,
    "description": "A showcase of AI kickstart examples for Red Hat OpenShift AI",
    "homepage": "https://erwangranger.github.io/quickstart",
    "repository": {
        "type": "git",
        "url": "https://github.com/erwangranger/AI-Kickstart-rendering"
    },
    "dependencies": {
        "@testing-library/user-event": "^13.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^9.0.1",
        "react-router-dom": "^7.9.1",
        "react-scripts": "5.0.1",
        "remark-gfm": "^4.0.0",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "eslint": "^8.57.1",
        "husky": "^9.1.7",
        "jest": "^27.5.1",
        "lint-staged": "^16.1.4",
        "prettier": "^3.6.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "generate-static": "node scripts/generate-static.js",
        "clean": "rm -rf functions api",
        "prebuild": "npm run clean && npm run generate-static",
        "setup": "npm run clean && rm -rf node_modules && npm cache clean --force && npm ci",
        "dev": "npm run setup && npm start",
        "build:local": "npm run setup && npm run build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "overrides": {
        "postcss": "^8.4.35",
        "nth-check": "^2.1.1",
        "semver": "^7.5.4"
    }
}
